[env]
TERM = "xterm-256color"

[font]
size = 14

[font.bold]
family = "Non-Seasonal Sans Mono"
style = "Regular"

[font.bold_italic]
family = "Non-Seasonal Sans Mono"
style = "Italic"

[font.italic]
family = "Non-Seasonal Sans Mono"
style = "Italic"

[font.normal]
family = "Non-Seasonal Sans Mono"
style = "Regular"

[[keyboard.bindings]]
action = "Paste"
key = "V"
mods = "Alt"

[[keyboard.bindings]]
action = "Copy"
key = "C"
mods = "Alt"

[primary]
background = "#272e33"
foreground = "#d3c6aa"

[primary.normal]
black = "#414b50"
blue = "#7fbbb3"
cyan = "#83c092"
green = "#a7c080"
magenta = "#d699b6"
red = "#e67e80"
white = "#d3c6aa"
yellow = "#dbbc7f"

[primary.normal.bright]
black = "#475258"
blue = "#7fbbb3"
cyan = "#83c092"
green = "#a7c080"
magenta = "#d699b6"
red = "#e67e80"
white = "#d3c6aa"
yellow = "#dbbc7f"

[scrolling]
history = 1000
multiplier = 1

[selection]
save_to_clipboard = true
semantic_escape_chars = ",â”‚`|:\"' ()[]{}<>\t"

[window]
dynamic_padding = false
opacity = 1
startup_mode = "Windowed"

[window.padding]
x = 5
y = 5
