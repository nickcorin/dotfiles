{
    "auto_update": true,
    "buffer_font_family": "Monaspace Argon",
    "buffer_font_size": 13,
    "current_line_highlight": "gutter",
    "cursor_blink": false,
    "ensure_final_newline_on_save": true,
    "format_on_save": "on",
    "git": {
        "git_gutter": "tracked_files"
    },
    "git_diff": true,
    "git_status": true,
    "theme": "Gruvbox Dark Hard",
    "tab_size": 4,
    "vim_mode": true
}
