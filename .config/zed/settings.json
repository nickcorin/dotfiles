{
    "assistant": {
        "default_model": {
            "provider": "copilot_chat",
            "model": "claude-3-5-sonnet"
        },
        "version": "2"
    },
    "auto_update": true,
    "buffer_font_family": "Monaspace Neon",
    "buffer_font_size": 14,
    "current_line_highlight": "gutter",
    "cursor_blink": false,
    "ensure_final_newline_on_save": true,
    "format_on_save": "on",
    "git": {
        "git_gutter": "tracked_files"
    },
    "git_diff": true,
    "git_status": true,
    "theme": "Ros√© Pine",
    "tab_size": 4,
    "vim_mode": true
}
